{
    "name": "memotable",
    "version": "3.1.5",
    "description": "A tiny JavaScript data structure for maintaining derived collections (indexed and sorted) with correct incremental updates.",
    "type": "module",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./react": {
            "types": "./dist/integrations/react/index.d.ts",
            "import": "./dist/integrations/react/index.js",
            "require": "./dist/integrations/react/index.cjs"
        }
    },
    "types": "./dist/index.d.ts",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "dev": "tsup src/index.ts --watch",
        "demo": "vite examples/react",
        "build": "tsup src/index.ts src/integrations/react/index.tsx --format cjs,esm --dts --minify --clean",
        "test": "vitest run --coverage",
        "test:watch": "vitest",
        "lint": "eslint .",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "pnpm lint && pnpm typecheck && pnpm test && pnpm build",
        "release": "pnpm changeset version && pnpm install && git add . && git commit -m 'chore: version bump' && git push",
        "publish:ci": "pnpm changeset publish",
        "prepare": "husky install",
        "size": "pnpm build && pnpm size-limit && node measure-size.ts memotable Table && pnpm size:mangle",
        "size:mangle": "npx esbuild src/index.ts --bundle --minify --mangle-props='^_' --outfile=dist/mangled.js && brotli -c dist/mangled.js | wc -c",
        "benchmark": "vitest run Table.benchmark.test.ts --testTimeout=120000 --no-coverage",
        "ci": "pnpm format && pnpm lint && pnpm format:check && pnpm typecheck && pnpm test && pnpm size"
    },
    "keywords": [
        "typescript",
        "memoization",
        "incremental",
        "in-memory",
        "indexed-collections",
        "sorted-collections",
        "filtered-collections",
        "derived-data",
        "reactive-data-structure",
        "data-indexing",
        "materialized-view",
        "batching",
        "partitioning",
        "usememo-alternative"
    ],
    "author": "Shubham Dwivedi <shudv@users.noreply.github.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/shudv/memotable.git"
    },
    "bugs": {
        "url": "https://github.com/shudv/memotable/issues"
    },
    "homepage": "https://github.com/shudv/memotable#readme",
    "devDependencies": {
        "@changesets/cli": "^2.29.7",
        "@eslint/js": "^9.39.1",
        "@size-limit/preset-small-lib": "^11.2.0",
        "@testing-library/react": "^14.3.1",
        "@types/node": "^24.10.1",
        "@types/react": "^18.3.27",
        "@types/react-dom": "^18.3.7",
        "@typescript-eslint/eslint-plugin": "^8.47.0",
        "@typescript-eslint/parser": "^8.47.0",
        "@vitejs/plugin-react": "^4.7.0",
        "@vitest/coverage-v8": "^4.0.10",
        "esbuild": "^0.25.12",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "husky": "^9.1.7",
        "jsdom": "^24.1.3",
        "lint-staged": "^16.2.6",
        "prettier": "^3.6.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "size-limit": "^11.2.0",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.47.0",
        "vite": "^5.4.21",
        "vitest": "^4.0.10"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{js,json,md,css,html}": [
            "prettier --write"
        ]
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "size-limit": [
        {
            "name": "Table",
            "path": "dist/index.js",
            "import": "{ Table }",
            "limit": "1500 B"
        }
    ],
    "packageManager": "pnpm@10.22.0",
    "peerDependencies": {
        "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        }
    }
}
